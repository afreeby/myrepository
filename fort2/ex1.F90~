program control_structures

implicit none
integer :: innum
integer, parameter :: fizz = 3, buzz = 5
print *, "Enter an integer: "
read *, innum

if(innum%fizz == 0 .and. innum%buzz == 0) then
   print *, "FizzBuzz"
else if(innum%fizz == 0) then
   print *, "Fizz"
else if(innum%buzz == 0) then
   print *, "Buzz"
end if

end program control_structures
